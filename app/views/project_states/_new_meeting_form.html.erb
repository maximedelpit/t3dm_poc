<!-- USELESS -->
<%= simple_form_for meeting, remote: true do |f| %>
  <%= f.error_notification %>
  <%#= date_field_tag 'project[date]', @project.due_date, class: 'datepicker' %>

  <%= f.input :date, as: :string, input_html: { class: 'datepicker', value: @ref_date }, wrapper_html: { class: 's6' } %>
  <%= f.input :time, as: :string, input_html: { class: 'clockpicker', value: @ref_time }, wrapper_html: { class: 's6' } %>
  <%= f.input :project_id, as: :hidden, input_html: { value: @meeting.project_id } %>
  <%= f.input :object, as: :hidden, input_html: { value: "Setup" } %>
  <%= f.button :button, class:'btn', wrapper_html: { class: 's6' }, data: {disable_with: "<i class='fa fa-spinner fa-spin'></i> Saving..."} %>
<% end %>
