
  <li data-sha='<%= data.sha %>' data-url='<%= data.url %>' data-type='<%= data.type %>'><%= data.name %>
    <% if subdata != {} %>
      <% subdata.each do | sub, value| %>
        <ul>
          <li data-sha='<%= sub.sha %>' sub-url='<%= sub.url %>' sub-type='<%= sub.type %>'><%= sub.name %></li>
        </ul>
      <% end %>
    <% end %>
  </li>

